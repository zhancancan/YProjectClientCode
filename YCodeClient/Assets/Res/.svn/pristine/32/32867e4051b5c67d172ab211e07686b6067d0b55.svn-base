local class=require "Class"
local this =class("Data_CmdDestroyObject",require "Data_Action")
local log = require "Logger"

-- - is self
function this:execute(_)
	local ids = self.ids;
	if not ids then return end
	for i=1 ,# ids do
		local id = ids[i]
		if id ~=self.char.id then self.table:remove(id) end
	end
end

return this