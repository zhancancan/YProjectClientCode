local this = {} ;
local TestLuaAction= this;
function this:enter(c) 
	self.action =c;
	self.time=false; 
	self.running=true;
end
function this:update(now)
	if self.time==false then self.time=now;	end
	if now - self.time >1000 then self.running =false; end
end
 
function this:running()
	return self.running;
end
return this;