local class 	= require "Class"
local base 		= require "Data_Action"
local this 		= class("CmdMoveObject",base)
local user 		= require "Data_Character"

function this:ctor()
	self.time = 0
end
-- - is self
function this:execute(_)
	if self.char.id == user.id then return end
	self.stateName= "Move"
	local d =self.table:selectOne(self.id)
	if d and d.entity then
		d.entity:ChangeState(self)
	end
end

return this