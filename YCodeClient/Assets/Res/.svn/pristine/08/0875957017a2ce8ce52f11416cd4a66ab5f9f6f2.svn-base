local class 		= require "Class"
local base			= require "ModelProxy"
local this 			= class("Proxy_Npc",base)
local namespace 	= require "CSNamespace"
local db 			= require "TypeDB"


function this:pushData(buffer,_)
	local isNew,data=self:pushData(buffer)
	if not data then return end

	if isNew then
		data._data.isHost = false
		-- data._data.ai = ""

		local model =namespace["Data_ModelLua"].new
		local o = model(data)
		data.model = o
	end
	
end

return this